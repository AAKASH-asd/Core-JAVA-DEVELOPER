import java.io.*;
import java.util.*;

public class Main{

public static void main(String[] args) throws Exception {
Scanner scn=new Scanner(System.in);
int n=scn.nextInt();
int arr[]=new int[n];


for(int i=0;i<n;i++){
    arr[i]=scn.nextInt();
}

int element=scn.nextInt();



for(int i =0;i<n;i++)
     {  
         if(arr[i]==element)
         {  
             
              System.out.println(i);
              return 0;

            
         }

 }
 System.out.println(-1);
}
}
